<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_selected="true">
        <shape android:shape="oval">
            <solid android:color="@color/color_primary_green_normal"/>
            <stroke android:width="@dimen/size_primary_border_width" android:color="@color/color_primary_green_normal"/>
        </shape>
    </item>

    <item>
        <shape android:shape="oval">
            <solid android:color="@color/color_primary_indicator_normal"/>
            <stroke android:width="@dimen/size_primary_border_width" android:color="@color/color_primary_indicator_normal"/>
        </shape>
    </item>

</selector>